{
    "name": "Yoghi/Madda",
    "license": "GPLv3",
    "type": "symfony-bundle",
    "description": "Model And Domain Driven Architecture",
    "keywords": ["MDA", "DDD", "generator"],
    "homepage": "https://github.com/yoghi/madda/tree/master",
    "authors": [
        {
            "name": "Stefano Tamagnini",
            "homepage": "http://www.sigmalab.net"
        }
