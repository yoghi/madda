{
    "name": "yoghi/madda",
    "license": "GPLv3",
    "type": "application",
    "description": "Model And Domain Driven Architecture",
    "keywords": ["MDA", "DDD", "generator"],
    "homepage": "https://github.com/yoghi/madda/tree/master",
    "authors": [
        {
            "name": "Stefano Tamagnini",
            "homepage": "http://www.sigmalab.net"
        }
    ],
    "autoload": {
        "psr-4": { "Yoghi\\Bundle\\MaddaBundle\\": "" , "Yoghi\\Bundle\\MaddaBundleTest\\": "Tests" }
    },
    "prefer-stable": true,
    "require": {
        "php": ">=5.6",
        "psr/log": "^1.0",
        "symfony/finder": "^3.0",
        "symfony/console": "^3.0",
        "symfony/process": "^3.0",
        "symfony/filesystem": "^3.0",
        "webpatser/laravel-uuid": "2.*",
        "nette/php-generator": "^2.3",
        "friendsofphp/php-cs-fixer": "^1.11",
        "bocharsky-bw/arrayzy": "^0.6.0",
        "league/flysystem": "^1.0",
        "cweagans/composer-patches": "1.5.0"
    },
    "require-dev" : {
        "sensiolabs/security-checker": "^3.0",
        "codeclimate/php-test-reporter": "^0.3.2",
        "phpunit/phpunit": "5.4.*",
        "phpunit/php-code-coverage": "^4.0",
        "satooshi/php-coveralls": "^1.0",
        "mikey179/vfsStream": "^1.6",
        "monolog/monolog": "^1.19",
        "johnkary/phpunit-speedtrap": "^1.0"
    },
    "scripts": {
    },
    "extra": {
      "patches": {
        "nette/utils": {
          "Fix HHVM Callback": "Resources/patch/nette-utils-callback.patch"
        }
      }
    },
    "bin": ["madda"]
}
